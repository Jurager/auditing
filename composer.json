{
    "name": "jurager/auditing",
    "description": "Audit changes of your Eloquent models in Laravel/Lumen",
    "keywords": [
        "accountability",
        "audit",
        "auditing",
        "changes",
        "eloquent",
        "history",
        "log",
        "logging",
        "observer",
        "laravel",
        "lumen",
        "record",
        "revision",
        "tracking"
    ],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Antério Vieira",
            "email": "anteriovieira@gmail.com"
        },
        {
            "name": "Quetzy Garcia",
            "email": "quetzyg@altek.org"
        },
        {
            "name": "Raphael França",
            "email": "raphaelfrancabsb@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0",
        "laravel/helpers": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Jurager\\Auditing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jurager\\Auditing\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jurager\\Auditing\\AuditingServiceProvider"
            ]
        }
    }
}
